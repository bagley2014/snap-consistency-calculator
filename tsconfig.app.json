{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.ts",
		"src/**/*.vue"
	],
	"exclude": [
		"src/**/__tests__/*"
	],
	"files": [
		"src/assets/marvel-snap-cards/data.json"
	],
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	}
}